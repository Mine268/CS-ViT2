{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [

        {
            "name": "Python Debugger: Current File",
            "type": "debugpy",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal"
        },

        {
            "name": "test_DinoBackbone",
            "type": "debugpy",
            "request": "launch",
            "module": "test.test_src_model_module",
            "console": "integratedTerminal"
        },


        {
            "name": "test_PoseNet",
            "type": "debugpy",
            "request": "launch",
            "module": "test.test_src_model_net",
            "console": "integratedTerminal"
        },

        {
            "name": "stage1",
            "type": "debugpy",
            "request": "launch",
            "module": "accelerate.commands.launch",
            "console": "integratedTerminal",
            "args": [
                "--num_processes", "1",
                "--num_machines", "1",
                "--mixed_precision", "no",
                "-m", "script.stage1",
                "--config-name", "default_stage1-handec-large",
                "AIM.server_url=.",
                "TRAIN.sample_per_device=36",
                "MODEL.joint_type=\"3\""
            ],
            "justMyCode": true,
            "env": {
                "CUDA_VISIBLE_DEVICES": "3"
            }
        }

    ]
}