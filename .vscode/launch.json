    "version": "0.2.0",
    "configurations": [

        {
            "name": "Python Debugger: Current File",
            "type": "debugpy",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal"
        },

        {
            "name": "test_DinoBackbone",
            "type": "debugpy",
            "request": "launch",
            "module": "test.test_src_model_module",
            "console": "integratedTerminal"
        },

        {
            "name": "test_PoseNet",
            "type": "debugpy",
            "request": "launch",
            "module": "test.test_src_model_net",
            "console": "integratedTerminal"
        },

        {
            "name": "test_dataloader",
            "type": "debugpy",
            "request": "launch",
            "module": "tests.test_src_data_dataloader",
            "console": "integratedTerminal"
        },

        {
            "name": "stage1",
            "type": "debugpy",
            "request": "launch",
            "module": "script.train",
            "console": "integratedTerminal",
            "args": [
                "--config-name", "stage1-dino_large",
                "AIM.server_url=.",
                "GENERAL.description='debug run'",
                "TRAIN.sample_per_device=32",
                "GENERAL.resume_path=checkpoint/09-02-2026/16-29-57_stage1-dino_large/checkpoints/checkpoint-12000",
                "augmentation=aggressive"
            ],
            "justMyCode": true,
            "env": {
                "CUDA_VISIBLE_DEVICES": "0",
                "CUDA_DEVICE_ORDER": "PCI_BUS_ID"
            }
        },

        {
            "name": "stage2",
            "type": "debugpy",
            "request": "launch",
            "module": "script.train",
            "console": "integratedTerminal",
            "args": [
                "--config-name", "stage2-dino_large",
                "AIM.server_url=.",
                "GENERAL.description='debug run'",
                "TRAIN.sample_per_device=4",
            ],
            "justMyCode": true,
            "env": {
                "CUDA_VISIBLE_DEVICES": "0",
                "CUDA_DEVICE_ORDER": "PCI_BUS_ID"
            }
        }


    ]
}