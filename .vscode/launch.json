    "version": "0.2.0",
    "configurations": [

        {
            "name": "Python Debugger: Current File",
            "type": "debugpy",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal"
        },

        {
            "name": "test_DinoBackbone",
            "type": "debugpy",
            "request": "launch",
            "module": "test.test_src_model_module",
            "console": "integratedTerminal"
        },

        {
            "name": "test_PoseNet",
            "type": "debugpy",
            "request": "launch",
            "module": "test.test_src_model_net",
            "console": "integratedTerminal"
        },

        {
            "name": "test_dataloader",
            "type": "debugpy",
            "request": "launch",
            "module": "test.test_src_data_dataloader",
            "console": "integratedTerminal"
        },

        {
            "name": "stage1",
            "type": "debugpy",
            "request": "launch",
            "module": "script.stage1",
            "console": "integratedTerminal",
            "args": [
                "--config-name", "stage1-dino_large",
                "AIM.server_url=.",
                "GENERAL.description='debug run'",
                "TRAIN.sample_per_device=32",
            ],
            "justMyCode": true,
            "env": {
                "CUDA_VISIBLE_DEVICES": "0",
                "CUDA_DEVICE_ORDER": "PCI_BUS_ID"
            }
        }

    ]
}