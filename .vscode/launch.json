    "version": "0.2.0",
    "configurations": [

        {
            "name": "Python Debugger: Current File",
            "type": "debugpy",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal"
        },

        {
            "name": "test_DinoBackbone",
            "type": "debugpy",
            "request": "launch",
            "module": "test.test_src_model_module",
            "console": "integratedTerminal"
        },


        {
            "name": "test_PoseNet",
            "type": "debugpy",
            "request": "launch",
            "module": "test.test_src_model_net",
            "console": "integratedTerminal"
        },

        {
            "name": "stage1",
            "type": "debugpy",
            "request": "launch",
            "module": "accelerate.commands.launch",
            "console": "integratedTerminal",
            "args": [
                "--num_processes", "1",
                "--num_machines", "1",
                "--mixed_precision", "no",
                "--main_process_port", "29504",
                "-m", "script.stage1",
                "--config-name", "stage1-dino_large-frz.yaml",
                "AIM.server_url=.",
                "TRAIN.sample_per_device=16",
                "MODEL.joint_type=\"3\"",
                "MODEL.handec.denorm_output=false",
                "GENERAL.resume_path=checkpoint/12-01-2026/16-57-14_stage1-dino_large-frz.yaml/checkpoints/checkpoint-24000",
                "TRAIN.backbone_lr=null"
            ],
            "justMyCode": true,
            "env": {
                "CUDA_VISIBLE_DEVICES": "1",
                "CUDA_DEVICE_ORDER": "PCI_BUS_ID"
            }
        }

    ]
}